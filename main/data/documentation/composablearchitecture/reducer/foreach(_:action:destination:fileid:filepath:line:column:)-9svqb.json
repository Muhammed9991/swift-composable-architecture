e {","    var path = StackState<Path.State>()","    \/\/ ...","  }","  enum Action {","    case path(StackActionOf<Path>)","    \/\/ ...","  }","  var body: some ReducerOf<Self> {","    Reduce { state, action in","      \/\/ Core parent logic","    }","    .forEach(\\.path, action: \\.path) {","      Path()","    }","  }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" operator does a number of things to make integrating parent and child features"},{"type":"text","text":" "},{"type":"text","text":"ergonomic and enforce correctness:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"text":"It forces a specific order of operations for the child and parent features:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"When a ","type":"text"},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/element(id:action:)","type":"reference","isActive":true},{"text":" action is sent it runs the","type":"text"},{"text":" ","type":"text"},{"text":"child first, and then the parent. If the order was reversed, then it would be possible","type":"text"},{"text":" ","type":"text"},{"text":"for the parent feature to ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" out the child state, in which case the child feature","type":"text"},{"text":" ","type":"text"},{"text":"would not be able to react to that action. That can cause subtle bugs.","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"When a "},{"isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/popFrom(id:)","type":"reference"},{"type":"text","text":" action is sent it runs the parent feature"},{"type":"text","text":" "},{"type":"text","text":"before the child state is popped off the stack. This gives the parent feature an"},{"type":"text","text":" "},{"type":"text","text":"opportunity to inspect the child state one last time before the state is removed."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"When a ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/push(id:state:)"},{"text":" action is sent it runs the parent feature","type":"text"},{"text":" ","type":"text"},{"text":"after the child state is appended to the stack. This gives the parent feature an","type":"text"},{"text":" ","type":"text"},{"text":"opportunity to make extra mutations to the state after it has been added.","type":"text"}]}]}]}]},{"content":[{"inlineContent":[{"text":"It automatically cancels all child effects when it detects the child’s state is removed","type":"text"},{"text":" ","type":"text"},{"text":"from the stack","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"It gives the child feature access to the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DismissEffect","type":"reference"},{"text":" dependency, which allows the","type":"text"},{"text":" ","type":"text"},{"text":"child feature to dismiss itself without communicating with the parent.","type":"text"}],"type":"paragraph"}]}]}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"sections":[],"seeAlsoSections":[{"anchor":"Stack-based-navigation","title":"Stack-based navigation","identifiers":["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackBasedNavigation","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackState","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackActionOf","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackE