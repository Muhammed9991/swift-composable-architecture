ier"},{"text":", ","kind":"text"},{"text":"Element","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP","text":"Reducer","kind":"typeIdentifier"},{"text":"\n","kind":"text"}],"platforms":["macOS"]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"text":"A writable key path from parent state to an ","type":"text"},{"code":"IdentifiedArray","type":"codeVoice"},{"text":" of child","type":"text"},{"text":" ","type":"text"},{"text":"state.","type":"text"}],"type":"paragraph"}],"name":"toElementsState"},{"content":[{"inlineContent":[{"type":"text","text":"A case path from parent action to an "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/IdentifiedAction","type":"reference","isActive":true},{"type":"text","text":" of child"},{"type":"text","text":" "},{"type":"text","text":"actions."}],"type":"paragraph"}],"name":"toElementAction"},{"content":[{"inlineContent":[{"type":"text","text":"A reducer that will be invoked with child actions against elements of child"},{"type":"text","text":" "},{"type":"text","text":"state."}],"type":"paragraph"}],"name":"element"},{"content":[{"type":"paragraph","inlineContent":[{"text":"The fileID.","type":"text"}]}],"name":"fileID"},{"content":[{"inlineContent":[{"type":"text","text":"The filePath."}],"type":"paragraph"}],"name":"filePath"},{"content":[{"type":"paragraph","inlineContent":[{"text":"The line.","type":"text"}]}],"name":"line"},{"content":[{"type":"paragraph","inlineContent":[{"text":"The column.","type":"text"}]}],"name":"column"}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}],"kind":"content"},{"content":[{"text":"Discussion","anchor":"discussion","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"For example, if a parent feature holds onto an array of child states, then it can perform"},{"type":"text","text":" "},{"type":"text","text":"its core logic "},{"type":"emphasis","inlineContent":[{"text":"and","type":"text"}]},{"type":"text","text":" the childâ€™s logic by using the "},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" operator:"}]},{"code":["@Reducer","struct Parent {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case rows(IdentifiedActionOf<Row>)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \\.rows) {","      Row()","    }","  }","}"],"syntax":"swift","type":"codeListing"},{"content":[{"type":"paragraph","inlineContent":[{"text":"We are using ","type":"text"},{"code":"IdentifiedArray","type":"codeVoice"},{"text":" from our","type":"text"},{"text":" ","type":"text"},{"identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","isActive":true,"type":"reference"},{"text":" library because it provides a safe","type":"text"},{"text":" ","type":"text"},{"text":"and ergonomic API for accessing elements from a stable ID rather than positional indices.","type":"text"}]}],"style":"tip","type":"aside","name":"Tip"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"type":"text","text":" "},{"type":"text","text":"runs the child first, and then the parent. If the order was reversed, then it would be"},{"type":"text","text":" "},{"type":"text","text":"possible for the parent feature to remove the child state from the array, in which case the"},{"type":"text","text":" "},{"type":"text","text":"child feature would not be able to react to that action. That can cause subtle bugs."}]},{"type":"paragraph","inlineContent":[{"text":"It is still possible for a parent feature higher up in the application to remove the child","type":"text"},{"text":" ","type":"text"},{"text":"state from the array before the child has a chance to react to the action. In such cases a","type":"text"},{"text":" ","type":"text"},{"text":"runtime warning is shown in Xcode to let you know that thereâ€™s a potential problem.","type":"text"}]}],"kind":"content"}],"schemaVersion":{"patch":0,"minor":3,"major":0},"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reduce","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reduce\/Reducer-Implementations"]]},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/composablearchitecture\/reduce\/foreach(_:action:element:fileid:filepath:line:column:)-4hrh1"]}],"metadata":{"title":"forEach(_:action:element:fileID:filePath:line:column:)","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"forEach","kind":"identifier"},{"text":"<","kind":"text"},{"text":"ElementState","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"ElementAction","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"ID","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"Element","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"WritableKeyPath","preciseIdentifier":"s:s15WritableKeyPathC","kind":"typeIdentifier"},{"text":"<","kind":"text"},{"text":"Self","kind":"typeIdentifier"},{"text":".","kind":"text"},{"text":"State","preciseIdentifier":"s:22ComposableArchitecture7ReducerP5StateQa","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"t